{
  "name": "agent-dbg",
  "version": "0.1.1",
  "description": "Node.js Debugger CLI for AI Agents",
  "type": "module",
  "bin": {
    "agent-dbg": "./dist/main.js"
  },
  "scripts": {
    "dev": "bun run src/main.ts",
    "build": "bun build src/main.ts --outdir dist",
