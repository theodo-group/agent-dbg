    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.14",
    "@types/bun": "latest",
    "devtools-protocol": "^0.0.1581282"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@jridgewell/trace-mapping": "^0.3.31",
    "zod": "^4.0.0"
  },
  "module": "index.ts"
}
